    "jest-styled-components": "^5.0.1",
    "prop-types": "^15.6.1",
    "react": "16.3.0-alpha.1",
    "react-dom": "^16.3.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-26.0.0.tar.gz",
    "react-native-svg-image": "^2.0.1",
    "react-navigation": "^1.5.11",
    "react-test-renderer": "^16.3.2",
    "shevyjs": "^1.1.0",
